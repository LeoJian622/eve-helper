# Eve Helper 环境变量配置模板
# 复制此文件为 .env 并填入实际值

# 数据库连接配置
DB_HOST=localhost
DB_PORT=3306
DB_SYSTEM_USERNAME=your_username
DB_SYSTEM_PASSWORD=your_system_db_password
DB_EVE_USERNAME=your_username
DB_EVE_PASSWORD=your_eve_db_password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# JWT密钥库配置
KEYSTORE_PASSWORD=your_keystore_password
KEY_PASSWORD=your_key_password
